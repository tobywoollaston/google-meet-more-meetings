{
    "name": "GoogleMeet More Meetings",
    "version": "0.1.0",
    "description": "Showing more google meet meetings",
    "key": "",
    "permissions": ["identity", "storage", "tabs"],
    "host_permissions": ["https://meet.google.com/*", "https://www.googleapis.com/oauth2/*"],
    "oauth2": {
        "client_id": "1048001560168-t1sik8s4qjtt94qvh01oqk4otlv4qolf.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/calendar.readonly"]
    },
    "background": {
        "service_worker": "src/page/background.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["src/page/content.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "src/page/main.js",
                "src/common.js",
                "src/dateFormat.js",
                "src/dateHelpers.js",
                "src/googleApi.js"
            ],
            "matches": ["https://meet.google.com/*"]
        }
    ],
    "action": {
        "default_icon": {
            "16": "assets/ext-icon.png",
            "24": "assets/ext-icon.png",
            "32": "assets/ext-icon.png"
        },
        "default_title": "GoogleMeet More Meetings",
        "default_popup": "src/popup/popup.html"
    },
    "manifest_version": 3
}
